# Azure Event Grid Configuration
# Copy this file to .env and fill in your actual values

# Event Grid Topic Endpoint
EVENT_GRID_ENDPOINT=https://your-topic.eventgrid.azure.net/api/events

# Event Grid Access Key
EVENT_GRID_ACCESS_KEY=your_access_key_here

# Azure Storage Account (if needed)
AZURE_STORAGE_CONNECTION_STRING=your_storage_connection_string_here
